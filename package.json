{
  "name": "air-tickets-web",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build-ios": "set NODE_ENV=production && npx react-native bundle --entry-file App.tsx --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "set NODE_ENV=production && npx react-native bundle --entry-file App.tsx --platform android --dev false --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/"
  },
  "dependencies": {
    "@ant-design/icons-react-native": "^2.3.2",
    "@ant-design/react-native": "^5.1.1",
    "@expo/metro-runtime": "~3.1.3",
    "@react-native/metro-config": "^0.75.0-main",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "expo": "~50.0.14",
    "expo-font": "^11.10.3",
    "expo-splash-screen": "^0.26.4",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "babel-plugin-module-resolver": "^5.0.0",
    "metro": "^0.80.8",
    "metro-core": "^0.80.8",
    "metro-react-native-babel-preset": "^0.77.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
